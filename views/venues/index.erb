<div class="page-header">
  <h1>Venues</h1>
</div>

<div class="main-cell page-header space">
  <a href="/venues/new">Add New Venue</a>  
  <form method="get">
    <label for="search">Search Venue by Location: </label>
    <input type="text" name="search" value="" id="search">
    <input type="submit" value="Search">
  </form>
</div>

<div class="main-cell">
  <% @venues.each do |venue| %>
    <h3><a href="/venues/<%= venue.id %>"><%= venue.name %></a></h3>
  <% end %>
</div>