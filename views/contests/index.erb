<div class="page-header">
  <h1>Contests</h1>
</div>

<div class="main-cell medalists">
  <table>
    <tr>
      <th>Event</th>
      <th>Venue</th>
    <tr>
    <% @contests.each do |contest| %>
      <tr>
        <td><a href="/contests/<%= contest.id %>"><%= contest.full_name %></a></td>
        <td><a href="/venues/<%= contest.venue.id %>"><%= contest.venue.name %></a></td>
      </tr>
    <% end %>
  </table>
</div>